# Template de Variáveis de Ambiente
# Copie este arquivo para .env e preencha com seus valores reais

# Chave secreta para segurança da aplicação
# Gere uma chave segura usando: python generate_secret_key.py
SECRET_KEY=sua_chave_secreta_super_segura_aqui_123456789

# URL do banco de dados
# Para SQLite local: sqlite:///prestige_clinic.db
# Para PostgreSQL: postgresql://user:password@host:port/database
DATABASE_URL=sqlite:///prestige_clinic.db

# Credenciais do Google Sheets (JSON como string)
# 1. Acesse Google Cloud Console
# 2. Crie uma Service Account
# 3. Baixe o arquivo JSON
# 4. Converta para string JSON e cole aqui
GOOGLE_SHEETS_CREDENTIALS={"type": "service_account", "project_id": "seu_project_id", "private_key_id": "sua_private_key_id", "private_key": "-----BEGIN PRIVATE KEY-----\nsua_private_key_aqui\n-----END PRIVATE KEY-----\n", "client_email": "seu_email@seu_project.iam.gserviceaccount.com", "client_id": "seu_client_id", "auth_uri": "https://accounts.google.com/o/oauth2/auth", "token_uri": "https://oauth2.googleapis.com/token", "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs", "client_x509_cert_url": "https://www.googleapis.com/robot/v1/metadata/x509/seu_email%40seu_project.iam.gserviceaccount.com"}

# Configurações adicionais (opcional)
DEBUG=True
LOG_LEVEL=INFO
